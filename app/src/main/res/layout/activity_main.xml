<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <Button
        android:id="@+id/buttontrajet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="calcul du temps de trajet"
        app:layout_constraintTop_toBottomOf="@+id/destination_addresses"
        tools:layout_editor_absoluteX="76dp" />

    <EditText
        android:id="@+id/origin_addresses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:hint="Départ"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="87dp" />

    <EditText
        android:id="@+id/destination_addresses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:hint="Arrivée"
        app:layout_constraintTop_toBottomOf="@+id/origin_addresses"
        tools:layout_editor_absoluteX="87dp" />


</androidx.constraintlayout.widget.ConstraintLayout>